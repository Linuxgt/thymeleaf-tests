%EXTENDS ../springintegration.common
# ------------------------------------------------------------
%CONTEXT
a1 = true
a2 = false
b1 = null
b2 = 'one'
# ------------------------------------------------------------
%INPUT
<p th:text="${a1} ? (${b1} ?: ${b2})">...</p>
<p th:text="${a1} ? (${b1} ?: ${b2}) : (${b1} ?: ${b2})">...</p>
<p th:text="${a2} ? (${b1} ?: ${b2})">...</p>
<p th:text="${a2} ? (${b1} ?: ${b2}) : (${b2} ? 'two')">...</p>
# ------------------------------------------------------------
%OUTPUT
<p>one</p>
<p>one</p>
<p></p>
<p>two</p>
