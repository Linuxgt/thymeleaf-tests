%EXTENDS ../springintegration.common
# ------------------------------------------------------------
%CONTEXT
b1 = 'not null'
b2 = null
c1 = 2
c2 = 'two'
# ------------------------------------------------------------
%INPUT
<p th:text="${b1} ?: ${c1}">...</p>
<p th:text="${b2} ?: ${c1}">...</p>
<p th:text="${b1} ?: ${c2}">...</p>
<p th:text="${b2} ?: ${c2}">...</p>
# ------------------------------------------------------------
%OUTPUT
<p>not null</p>
<p>2</p>
<p>not null</p>
<p>two</p>
