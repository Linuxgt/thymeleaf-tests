%TEMPLATE_MODE HTML
# ------------------------------------------------------------
%EXACT_MATCH true
# ------------------------------------------------------------
%CONTEXT
data = {'alpha','beta'}
# ------------------------------------------------------------
%INPUT
<div>
    <p th:each="d : ${data}" th:text="${d}">...</p>
</div>
# ------------------------------------------------------------
%OUTPUT
<div>
    <p>alpha</p>
    <p>beta</p>
</div>
