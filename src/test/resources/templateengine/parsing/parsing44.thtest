%TEMPLATE_MODE HTML
# ------------------------------------------------------------
%EXACT_MATCH true
# ------------------------------------------------------------
%CONTEXT
data = {'alpha','beta'}
# ------------------------------------------------------------
%INPUT
<div>
    <span th:each="d : ${data}" th:text="${d}">...</span>
</div>
# ------------------------------------------------------------
%OUTPUT
<div>
    <span>alpha</span><span>beta</span>
</div>
