%TEMPLATE_MODE HTML
# ------------------------------------------------------------
%EXACT_MATCH true
# ------------------------------------------------------------
%CONTEXT
data = {'alpha','beta'}
# ------------------------------------------------------------
%INPUT
<br>
<ul th:each="l : ${data}">
    <li th:each="d : ${data}" th:text="${d}">
</ul>
# ------------------------------------------------------------
%OUTPUT
<br>
<ul>
    <li>alpha
    <li>beta</ul>
<ul>
    <li>alpha
    <li>beta</ul>
