%TEMPLATE_MODE HTML5
# ------------------------------------------------------------
%CONTEXT
onevar = 'something'
twovar = #{ 'value' : 25 }
# ------------------------------------------------------------
%INPUT
<div>
  <div th:object="${twovar}" th:assert="*{value} &gt; 25" class="a"
       th:remove="tag">tururu</div>
</div>
# ------------------------------------------------------------
%EXCEPTION org.thymeleaf.exceptions.TemplateAssertionException
%EXCEPTION_MESSAGE_PATTERN .*?\*\{value\}.*?
