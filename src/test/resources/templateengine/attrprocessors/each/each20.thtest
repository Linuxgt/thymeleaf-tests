%TEMPLATE_MODE TEXT
# ------------------------------------------------------------
%EXACT_MATCH true
# ------------------------------------------------------------
%INPUT
<table>[# th:each="num : ${#numbers.sequence(1,3)}"]
  <tr>
    <td>[[${num}]]</td>
  </tr>
[/]</table>
# ------------------------------------------------------------
%OUTPUT
<table>
  <tr>
    <td>1</td>
  </tr>
  <tr>
    <td>2</td>
  </tr>
  <tr>
    <td>3</td>
  </tr>
</table>
