%TEMPLATE_MODE TEXT
# ------------------------------------------------------------
%EXACT_MATCH true
# ------------------------------------------------------------
%INPUT
<table>
   _[# th:each="num : ${#numbers.sequence(1,2)}"]
  <tr>
    <td>[[${num}]]</td>
  </tr>
   [/]_
</table>
# ------------------------------------------------------------
%OUTPUT
<table>
   _
  <tr>
    <td>1</td>
  </tr>
  <tr>
    <td>2</td>
  </tr>
   _
</table>
