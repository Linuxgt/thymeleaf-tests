%TEMPLATE_MODE HTML5
# ------------------------------------------------------------
%CONTEXT
prices = #{ 'euros': 9.00,\
            'dollars': 12.00 }
product = #{ 'name': 'Lettuce',\
             'prices': prices }
# ------------------------------------------------------------
%INPUT
<p th:object="${product}" th:text="*{prices.euros}">...</p>
<p th:object="${product}">
  <span th:text="*{prices.euros}">...</span>
</p>
<p th:object="${product.prices}" th:text="*{euros}">...</p>
<p th:object="${product.prices}">
  <span th:text="*{euros}">...</span>
</p>
# ------------------------------------------------------------
%OUTPUT
<p>9.0</p>
<p>
  <span>9.0</span>
</p>
<p>9.0</p>
<p>
  <span>9.0</span>
</p>
