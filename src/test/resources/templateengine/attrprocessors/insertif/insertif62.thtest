%TEMPLATE_MODE HTML
# ------------------------------------------------------------
%INPUT[template]
<!DOCTYPE html>
<html xmlns:th="http://www.thymeleaf.org" th:fragment="page">
<head>
</head>
<body>
<span>Before</span>
<div th:insert-if="this::content">Content</div>
<span>After</span>
</body>
</html>
# ------------------------------------------------------------
%INPUT
<!DOCTYPE html>
<html xmlns:th="http://www.thymeleaf.org" th:insert-if="template :: page">
<head>
    <title></title>
</head>
<body>
<div th:fragment="content">Expected Content</div>
</body>
</html>
# ------------------------------------------------------------
%OUTPUT
<!DOCTYPE html>
<html><html>
<head>
</head>
<body>
<span>Before</span>
<div><div>Expected Content</div></div>
<span>After</span>
</body>
</html></html>
