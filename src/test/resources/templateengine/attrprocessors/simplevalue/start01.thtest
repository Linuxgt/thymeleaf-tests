%TEMPLATE_MODE HTML5
# ------------------------------------------------------------
%CONTEXT
one = 'one!'
# ------------------------------------------------------------
%INPUT
<div th:start="${one}">..</div>
<div th:start="'hello'">..</div>
<div th:start="${null}">..</div>
<div th:start="''">..</div>

<div start="old" th:start="${one}">..</div>
<div start="old" th:start="'hello'">..</div>
<div start="old" th:start="${null}">..</div>
<div start="old" th:start="''">..</div>
