<div action="old" data-th-action="${null}">..</div>
<div action="old" data-th-action="''">..</div>
# ------------------------------------------------------------
%OUTPUT
<div action="one!">..</div>
<div action="hello">..</div>
<div action="">..</div>
<div action="">..</div>

<div action="one!">..</div>
<div action="hello">..</div>
<div action="">..</div>
<div action="">..</div>
