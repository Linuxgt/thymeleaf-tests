%TEMPLATE_MODE HTML5
# ------------------------------------------------------------
%CONTEXT
prices = #{ 'euros': 9.00,\
            'dollars': 12.00 }
# ------------------------------------------------------------
%INPUT
<p th:object="${(prices.euros > 8.00)? prices.dollars : prices.euros}" th:text="${#object}">...</p>
# ------------------------------------------------------------
%OUTPUT
<p>12.0</p>
